{
  "name": "ReactNativeTemplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "react-native run-android --variant=debug",
    "android:staging": "react-native run-android --variant=staging",
    "android:release": "react-native run-android --variant=release",
    "android:ds:staging": "cd ./android/app/build/intermediates/merged_native_libs/staging/out/lib && zip -r ../../../../../../staging/native-debug-symbols.zip ./ -x \".DS_Store\" -x \"__MACOSX*\"",
    "android:ds:release": "cd ./android/app/build/intermediates/merged_native_libs/release/out/lib && zip -r ../../../../../../release/native-debug-symbols.zip ./ -x \".DS_Store\" -x \"__MACOSX*\"",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --scheme ReactNativeTemplateDev",
    "ios:staging": "react-native run-ios --scheme ReactNativeTemplateStaging",
    "ios:release": "react-native run-ios --scheme ReactNativeTemplateProd",
    "android:app-version-name": "appVersionName=$(cat android/app/build.gradle | grep \"def appVersionName\") && appVersionName=$(echo \"${appVersionName:20}\") && appVersionName=$(echo $appVersionName | sed 's/\"//g') && appVersionName=$(echo $appVersionName | sed 's/ //g') && echo $appVersionName",
    "codepush:all:staging": "npm run codepush:android:staging && npm run codepush:ios:staging",
    "codepush:all:production": "npm run codepush:android:production && npm run codepush:ios:production",
    "codepush:android:staging": "npm run lint && appVersionName=$(npm run --silent android:app-version-name) && appcenter codepush release-react -a app-center-username/app-center-app-id-android -d Staging -t $appVersionName",
    "codepush:android:production": "npm run lint && appVersionName=$(npm run --silent android:app-version-name) && appcenter codepush release-react -a app-center-username/app-center-app-id-android -d Production -t $appVersionName",
    "codepush:ios:staging": "npm run lint && appcenter codepush release-react -a app-center-username/app-center-app-id-ios -d Staging",
    "codepush:ios:production": "npm run lint && appcenter codepush release-react -a app-center-username/app-center-app-id-ios -d Production",
    "generate:bootsplash": "react-native generate-bootsplash src/@assets/bootsplash/logo.svg --platforms=android,ios --background=111111 --logo-width=100 --assets-output=assets --flavor=main --html=index.html",
    "lint": "eslint .",
    "start": "npm run watchman-del && npm run remove-ios-generated-dotenv-m && react-native start --no-interactive --reset-cache",
    "git-cache-reset": "git rm -r -f --cached . && git add .",
    "watchman-del": "watchman watch-del \"${PWD}\" ; watchman watch-project \"${PWD}\"",
    "remove-ios-generated-dotenv-m": "rm -f ./node_modules/react-native-config/ios/ReactNativeConfig/GeneratedDotEnv.m",
    "reinstall": "npm run reinstall:module && npm run reinstall:bundle && npm run reinstall:pod",
    "reinstall:module": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "reinstall:bundle": "rm -rf vendor && rm -f Gemfile.lock && bundle install",
    "reinstall:pod": "cd ios && rm -rf Pods && pod install --repo-update",
    "install:pod": "cd ios && pod install --repo-update",
    "postinstall": "patch-package",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.74.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/runtime": "7.24.7",
    "@invertase/react-native-apple-authentication": "2.3.0",
    "@notifee/react-native": "7.8.2",
