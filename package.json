{
  "name": "ReactNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "react-native run-android --variant=debug",
    "android:staging": "react-native run-android --variant=staging",
    "android:release": "react-native run-android --variant=release",
    "android:ds:staging": "cd ./android/app/build/intermediates/merged_native_libs/staging/out/lib && zip -r ../../../../../../staging/native-debug-symbols.zip ./ -x \".DS_Store\" -x \"__MACOSX*\"",
    "android:ds:release": "cd ./android/app/build/intermediates/merged_native_libs/release/out/lib && zip -r ../../../../../../release/native-debug-symbols.zip ./ -x \".DS_Store\" -x \"__MACOSX*\"",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --scheme ReactNativeTemplateDev",
    "ios:staging": "react-native run-ios --scheme ReactNativeTemplateStaging",
    "ios:release": "react-native run-ios --scheme ReactNativeTemplateProd",
    "android:app-version-name": "appVersionName=$(cat android/app/build.gradle | grep \"def appVersionName\") && appVersionName=$(echo \"${appVersionName:20}\") && appVersionName=$(echo $appVersionName | sed 's/\"//g') && appVersionName=$(echo $appVersionName | sed 's/ //g') && echo $appVersionName",
    "git:commit": "node .git-commit.js",
    "git:push": "git push",
    "git:commit:push": "npm run git:commit && npm run git:push",
    "git:merge:mirror": "node .git-merge.js mirror main",
    "generate:bootsplash": "react-native generate-bootsplash src/@assets/bootsplash/logo.svg --platforms=android,ios --background=100f11 --logo-width=100 --assets-output=assets --flavor=main --html=index.html",
    "asset": "node --stack-size=2048 $(which react-native-asset)",
    "lint": "eslint .",
    "tsc": "npx tsc --noEmit",
    "start": "npm run watchman-del && npm run remove-ios-generated-dotenv-m && react-native start --no-interactive --reset-cache",
    "git-cache-reset": "git rm -r -f --cached . && git add .",
    "watchman-del": "watchman watch-del \"${PWD}\" ; watchman watch-project \"${PWD}\"",
    "remove-ios-generated-dotenv-m": "rm -f ./node_modules/react-native-config/ios/ReactNativeConfig/GeneratedDotEnv.m",
    "update-ios": "eas update --channel=production-ios --message=$(date +%Y-%m-%d-%H-%M-%S)",
    "update-android": "eas update --channel=production-android --message=$(date +%Y-%m-%d-%H-%M-%S)",
    "reinstall": "npm run reinstall:module && npm run reinstall:pod",
    "reinstall:module": "rm -rf node_modules && rm -f package-lock.json && npm i",
    "reinstall:bundle": "rm -rf vendor && rm -f Gemfile.lock && bundle install",
    "reinstall:pod": "cd ios && rm -rf Pods && rm -f Podfile.lock && pod install --repo-update",
    "install:pod": "cd ios && pod install --repo-update",
    "postinstall": "patch-package",
    "test": "jest"
  },
  "dependencies": {
    "expo": "~53.0.0",
    "expo-updates": "~0.28.13",
    "react": "19.0.0",
    "react-native": "0.79.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@invertase/react-native-apple-authentication": "2.4.1",
    "@notifee/react-native": "9.1.8",
    "@pdg/api": "1.0.23",
    "@pdg/compare": "^1.0.6",
    "@pdg/data": "^1.0.1",
    "@pdg/date-time": "^1.0.2",
    "@pdg/formatting": "^1.0.5",
    "@pdg/react-hook": "1.0.31",
    "@pdg/types": "^1.0.2",
    "@react-native-community/blur": "4.4.1",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native-firebase/app": "22.2.0",
    "@react-native-firebase/auth": "22.2.0",
    "@react-native-firebase/crashlytics": "22.2.0",
    "@react-native-firebase/messaging": "22.2.0",
    "@react-native-google-signin/google-signin": "14.0.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-seoul/kakao-login": "5.4.1",
    "@react-native-seoul/naver-login": "4.1.2",
    "@react-native-vector-icons/ionicons": "12.0.0",
    "@react-native-vector-icons/lucide": "12.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@react-navigation/bottom-tabs": "7.3.14",
    "@react-navigation/elements": "2.4.3",
    "@react-navigation/native": "7.1.10",
    "@react-navigation/native-stack": "7.3.14",
    "@react-navigation/stack": "7.3.2",
    "@tsconfig/react-native": "3.0.5",
    "@types/jest": "29.5.14",
    "@types/react": "19.0.0",
    "@types/react-native": "0.73.0",
